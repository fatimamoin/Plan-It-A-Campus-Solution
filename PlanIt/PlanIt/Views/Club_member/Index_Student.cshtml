@model IEnumerable<PlanIt.Models.Club_member>

@{
    ViewBag.Title = "Index_Student";
    Layout = "~/Views/Shared/_StudentPage.cshtml";
}

<h2>Your Clubs</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Student.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Club.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Position.Name)
        </th>
        
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.Student.Name)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Club.Name)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Position.Name)
    </td>


</tr>
}

</table>
